{
  "name": "documentation",
  "version": "2.0.0",
  "description": "micro-lc & add-ons documentation",
  "license": "SEE LICENSE IN LICENSE",
  "packageManager": "yarn@3.6.1",
  "workspaces": [
    "applications/*"
  ],
  "scripts": {
    "cleanup": "rimraf build node_modules static/applications applications/react-browser-router/build applications/react-hash-router/build applications/react-browser-router/node_modules applications/react-hash-router/node_modules",
    "rbr": "yarn workspace @micro-lc/react-browser-router",
    "rhr": "yarn workspace @micro-lc/react-hash-router",
    "rmr": "yarn workspace @micro-lc/react-memory-router",
    "vue": "yarn workspace @micro-lc/vue3",
    "cspell": "cspell \"./docs/**\"",
    "lint:code": "eslint . --ext .mjs,.js,.jsx,.ts,.tsx --resolve-plugins-relative-to .",
    "lint:style": "stylelint \"**/*.{css}\" --allow-empty-input",
    "lint": "NODE_OPTIONS=--max-old-space-size=10000 yarn run-p lint:code lint:style",
    "build-rbr": "yarn rbr build",
    "build-rhr": "yarn rhr build",
    "build-rmr": "yarn rmr build",
    "build-vue": "yarn vue build",
    "build-applications": "run-p build-rbr build-rhr build-rmr build-vue",
    "build-static-frames": "ts-node scripts/interpolate-templates.ts",
    "build-docusaurus": "docusaurus build",
    "copy-static": "ts-node scripts/copy-static.ts",
    "prepare": "run-s build-applications build-vue copy-static build-static-frames",
    "start": "docusaurus start",
    "build": "run-s prepare build-docusaurus",
    "test:e2e": "playwright test",
    "tls:config": "ts-node scripts/tls-config.ts",
    "serve": "ts-node tests/server.ts",
    "check:types": "yarn tsc --noEmit && echo \"âœ“ check types\"",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@docusaurus/core": "^2.4.1",
    "@docusaurus/plugin-content-blog": "^2.4.1",
    "@docusaurus/plugin-content-docs": "^2.4.1",
    "@docusaurus/plugin-content-pages": "^2.4.1",
    "@docusaurus/theme-classic": "^2.4.1",
    "@docusaurus/theme-common": "^2.4.1",
    "@docusaurus/theme-search-algolia": "^2.4.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mdx-js/react": "1.6.22",
    "@micro-lc/post-channel": "^0.1.0",
    "@mui/material": "^5.14.1",
    "clsx": "^2.0.0",
    "js-yaml": "^4.1.0",
    "monaco-editor": "^0.40.0",
    "monaco-yaml": "^4.0.4",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rxjs": "^7.8.1",
    "strip-json-comments": "^5.0.1"
  },
  "devDependencies": {
    "@cspell/cspell-types": "^6.31.1",
    "@docusaurus/module-type-aliases": "^2.4.1",
    "@micro-lc/interfaces": "^1.1.0",
    "@playwright/test": "^1.35.0",
    "@tsconfig/docusaurus": "^2.0.0",
    "@types/eslint": "^8.37.0",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.5",
    "@types/json-schema": "^7.0.12",
    "@types/node": "^20.4.2",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^6.2.1",
    "confusing-browser-globals": "^1.0.11",
    "copy-webpack-plugin": "^11.0.0",
    "cspell": "^7.0.1",
    "docusaurus-plugin-remote-content": "^3.1.0",
    "docusaurus2-dotenv": "^1.4.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.42.0",
    "eslint-config-esnext": "^4.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "eslint-visitor-keys": "^3.4.2",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "glob": "^10.2.6",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "stylelint": "^15.9.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": ">=16.17.0"
  }
}
